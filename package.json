{
  "name": "cursor-playbook",
  "private": true,
  "type": "module",
  "scripts": {
    "lint": "eslint .",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "build": "echo 'noop build'",
    "test": "npm run unit && npm run smoke",
    "unit": "vitest run --silent --reporter=dot",
    "smoke": "playwright test tests/e2e/smoke.spec.ts --project=chromium",
    "prepush": "bash scripts/prepush.sh",
    "integrate": "node scripts/integrate.mjs --base=main --slug=$SLUG --branches=$BRANCHES",
    "agent:log": "node scripts/log-session.mjs"
  },
  "devDependencies": {
    "@playwright/test": "^1.46.0",
    "eslint": "^9.6.0",
    "typescript": "^5.5.4",
    "vitest": "^2.0.5"
  }
}


